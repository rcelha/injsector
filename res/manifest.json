{
  "manifest_version": 2,

  "name": "Injsector",
  "description": "JS Injector",
  "version": "1.0",
  "options_page": "popup.html",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [ {
    "js": [ "inject.js" ],
    "matches": [ "*://*/*"],
    "run_at": "document_start"
  }],
  "converted_from_user_script": true,
  "permissions": [
    "activeTab",
    "webRequest",
    "storage",
    "notifications",
    "*://*/"
  ]
}
